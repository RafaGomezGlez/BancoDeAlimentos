<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/feedFragment">

    <fragment
        android:id="@+id/mapaFragment"
        android:name="tec.mx.bancodecomida.mapa"
        android:label="Map"
        tools:layout="@layout/fragment_mapa" />
    <fragment
        android:id="@+id/milestonesFragment"
        android:name="tec.mx.bancodecomida.milestones"
        android:label="Milestones"
        tools:layout="@layout/fragment_milestones" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="tec.mx.bancodecomida.settings"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/feedFragment"
        android:name="tec.mx.bancodecomida.FeedFragment"
        android:label="Feed"
        tools:layout="@layout/fragment_feed">
        <argument
            android:name="password"
            app:argType="string" />
        <argument
            android:name="email"
            app:argType="string" />
        <argument
            android:name="name"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="tec.mx.bancodecomida.SignInFragment"
        android:label="Sign in"
        tools:layout="@layout/fragment_sign_in">
        <action
            android:id="@+id/action_signInFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
    </fragment>
    <fragment
        android:id="@+id/presentationFragment"
        android:name="tec.mx.bancodecomida.PresentationFragment"
        android:label="presentation"
        tools:layout="@layout/fragment_presentation">
        <action
            android:id="@+id/action_presentationFragment_to_logInFragment"
            app:destination="@id/logInFragment" />
        <action
            android:id="@+id/action_presentationFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/logInFragment"
        android:name="tec.mx.bancodecomida.LogInFragment"
        android:label="Login"
        tools:layout="@layout/fragment_log_in">
        <action
            android:id="@+id/action_logInFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
    </fragment>
</navigation>